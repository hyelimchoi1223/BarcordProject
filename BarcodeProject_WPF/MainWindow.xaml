<Window x:Class="BarcodeProject_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BarcodeProject_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid Margin="36,49,36,22.8">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="90"/>
                <ColumnDefinition/>
                <ColumnDefinition Width="150"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="30"/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>

        </Grid>
        <ToolBar Height="32" VerticalAlignment="Top" Margin="0,0,0.4,0">
            <Button x:Name="SettingBtn" Content="설정" VerticalAlignment="Stretch" Width="75" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Center" Margin="0,2" Click="SettingBtn_Click"/>
        </ToolBar>

    </Grid>
</Window>
